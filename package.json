{
    "name": "electron-cli",
    "version": "0.0.0",
    "private": true,
    "main": "main.js",
    "type": "module",
    "author": "lfsz",
    "description": "electron-cli",
    "scripts": {
        "dev": " vite --mode electron_dev",
        "dev:electron": "concurrently \"cross-env NODE_ENV=development vite --mode electron_dev\" \"wait-on https://localhost:8080 && cross-env NODE_ENV=development electron .\"",
        "mock": "vite --mode mock",
        "mock:electron": "concurrently \"cross-env NODE_ENV=development vite --mode electron_mock\" \"wait-on https://localhost:8080 && cross-env NODE_ENV=development electron .\"",
        "build": "vite build --mode dev",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "rimraf dist out && npm run build && electron-forge make",
        "make:ios": "rimraf dist out && npm run build && npm run make -- --platform=darwin",
        "make:win": "rimraf dist out && npm run build && npm run make -- --platform=win32"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "amfe-flexible": "^2.2.1",
        "axios": "^1.7.2",
        "electron-squirrel-startup": "^1.0.1",
        "element-plus": "^2.7.7",
        "jsencrypt": "^3.3.2",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "pinia": "^2.1.7",
        "qs": "^6.12.3",
        "vue": "^3.4.29",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.3.3"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.8.3",
        "@electron-forge/maker-deb": "^7.8.3",
        "@electron-forge/maker-rpm": "^7.8.3",
        "@electron-forge/maker-squirrel": "^7.8.3",
        "@electron-forge/maker-zip": "^7.8.3",
        "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
        "@electron-forge/plugin-fuses": "^7.8.3",
        "@electron/fuses": "^1.8.0",
        "@rushstack/eslint-patch": "^1.8.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-prettier": "^9.0.0",
        "concurrently": "^9.2.0",
        "cross-env": "^10.0.0",
        "electron": "^37.2.5",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "mockjs": "^1.1.0",
        "postcss-pxtorem": "^6.1.0",
        "prettier": "^3.2.5",
        "rimraf": "^6.0.1",
        "rollup-plugin-visualizer": "^5.12.0",
        "unplugin-auto-import": "^0.18.0",
        "unplugin-vue-components": "^0.27.3",
        "vite": "^5.3.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-mock": "^3.0.2",
        "vite-plugin-svg-icons": "^2.0.1",
        "wait-on": "^8.0.4"
    }
}
